{
    "apps": [
        {
            "name": "Devices",
            "script": "app.js",
            "instances": 4,
            "max_memory_restart": "450M",
            "exec_mode": "cluster",
            "log_file": "./logs/combined.outerr.log",
            "out_file": "./logs/out.log",
            "error_file": "./logs/err.log",
            "merge_logs": true,
            "log_date_format": "YYYY-MM-DD HH:mm Z",
            "watch": false,
            "trace": true,
            "env": {
                "NODE_ENV": "development",
                "PORT": 3030
            },
            "env_test": {
                "NODE_ENV": "test"
            },
            "env_staging": {
                "NODE_ENV": "staging"
            },
            "env_production": {
                "NODE_ENV": "production",
                "PORT": 3030,
                "SUPER_USERNAME": "",
                "SUPER_PASSWORD": ""
            }
        }
    ]
}